{
    "name": "margusk/getset",
    "description": "Automatic getters and setters for PHP classes",
    "type": "library",
    "keywords": ["getter", "setter", "getters", "setters"],
    "authors": [
        {
            "name": "Margus Kaidja",
            "email": "margusk@gmail.com"
        }
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    },
    "autoload": {
        "psr-4": {
            "margusk\\GetSet\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "margusk\\GetSet\\Tests\\": "tests/"
        }
    }
}
